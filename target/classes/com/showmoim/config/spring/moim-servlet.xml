<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="mocontrol" class="com.showmoim.controller.MoimController">
		<property name="moimService" ref="moservice"></property>
		<property name="moimMemberService" ref="momservice"></property>
	</bean>
	
	<bean id="momcontrol" class="com.showmoim.controller.MoimMemberController">
		<property name="moimService" ref="moservice"></property>
		<property name="moimMemberService" ref="momservice"></property>
	</bean>
	
	<bean id="irvr" class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="suffix" value=".jsp"></property>
	</bean>
</beans>
